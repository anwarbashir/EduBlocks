<category name="Charlie" colour="#ff0066" iconclass="blocklyTreeIconCustom charlie">

  <label text="Charlie" web-class="myLabelStyle"></label>

  <block type="start"></block>
  <block type="arrow1_on"></block>
  <block type="arrow1_off"></block>
  <block type="arrow2_on"></block>
  <block type="arrow2_off"></block>
  <block type="arrow3_on"></block>
  <block type="arrow3_off"></block>
  <block type="arrow4_on"></block>
  <block type="arrow4_off"></block>
  <block type="sonicCheck"></block>
  <block type="ls1Check"></block>
  <block type="ls2Check"></block>
  <block type="m1Forwardx"></block>
  <block type="m1Reversex"></block>
  <block type="m1Stop"></block>
  <block type="m2Forwardx"></block>
  <block type="m2Reversex"></block>
  <block type="m2Stop"></block>
  <block type="both_onWhiteLine"></block>
  <block type="ls1_onBlackLine"></block>
  <block type="ls2_onBlackLine"></block>
  <block type="try"></block>
  <block type="except"></block>
  <block type="GPIO_cleanup"></block>
  
  <block type="import_math"></block>
  <block type="random"></block>

  <label text="Statements" web-class="myLabelStyle"></label>
  <block type="sleepnew">
    <value name="sleep">
      <shadow type="textinline">
        <field name="text">1</field>
      </shadow>
    </value>
  </block>

  <block type="typeanything">
    <value name="stuff">
      <shadow type="textinline">
        <field name="text"></field>
      </shadow>
    </value>
  </block>
